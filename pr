    public setIABScript() {
        try {
            let firstScriptEl = window.document.getElementsByTagName('script')[2];
            const scriptEl = window.document.createElement('script');
            scriptEl.src = 'assets/third-party/sovrn.js';
            scriptEl.type = 'text/javascript';
            firstScriptEl.parentNode.insertBefore(scriptEl, firstScriptEl);

            firstScriptEl = window.document.getElementsByTagName('script')[3];
            const script = window.document.createElement('script');
            script.type = 'text/javascript';
            script.src = '//get.s-onetag.com/6c2e8c41-ac88-4fb0-9096-c24ee803454b/tag.min.js';
            script.async = true;
            script.defer = true;

            firstScriptEl.parentNode.insertBefore(script, firstScriptEl);
        } catch(err) {
          /* eslint-disable no-console */
          console.log('Loading IAB script failed')
          /* eslint-enable no-console */
        }
    }
