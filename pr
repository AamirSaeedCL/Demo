using Microsoft.SqlServer.Dac;

public class DacPacDeployer
{
    public void DeployDacPac(string dacPacPath, string connectionString, string databaseName)
    {
        DacServices dacServices = new DacServices(connectionString);
        DacPackage dacpac = DacPackage.Load(dacPacPath);
        dacServices.Deploy(dacpac, databaseName, upgradeExisting: true);
    }
}



using System;
using Microsoft.EntityFrameworkCore;

public class Program
{
    public static void Main()
    {
        string dacPacPath = @"path\to\your.dacpac";
        string connectionString = "your_connection_string";
        string databaseName = "YourDatabaseName";

        // Deploy DACPAC
        var deployer = new DacPacDeployer();
        deployer.DeployDacPac(dacPacPath, connectionString, databaseName);

        // Use EF Core
        using (var context = new YourDbContext())
        {
            var entity = new YourEntity { Name = "Test" };
            context.YourEntities.Add(entity);
            context.SaveChanges();
        }
    }
}

public class YourDbContext : DbContext
{
    public DbSet<YourEntity> YourEntities { get; set; }

    protected override void OnConfiguring(DbContextOptionsBuilder optionsBuilder)
    {
        optionsBuilder.UseSqlServer("your_connection_string");
    }
}

public class YourEntity
{
    public int Id { get; set; }
    public string Name { get; set; }
}
