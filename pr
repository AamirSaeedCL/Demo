SELECT ct.*
FROM cnsmr_tag ct
JOIN tag t
  ON t.tag_id = ct.tag_id
JOIN tag_typ tt
  ON tt.tag_typ_id = t.tag_typ_id
WHERE tt.tag_typ_shrt_nm IN (
    'DDRETCD',
    'DIAL_TAG',
    'DIGITAL',
    'PAYPHASE',
    'PHASE',
    'PLANSTAT',
    'PREDEF',
    'RMDTN',
    'SMS',
    'PLATSTAT'
)
AND ct.cnsmr_id = 1588200
AND ct.cnsmr_tag_sft_delete_flg = 'N';
