Query:

Select  t.tag_typ_id as typeId,count(*) as TotalRows
	from cnsmr_tag ct , tag t, tag_typ tt
	where tt.tag_typ_id = t.tag_typ_id
	AND t.tag_id = ct.tag_id
	AND tag_typ_shrt_nm IN ( 'DIAL_TAG' ) --- ,'DIAL_TAG','DIGITAL','PAYPHASE', 'PHASE',  'PLANSTAT', 'PREDEF', 'RMDTN', 'SMS', 'PLATSTAT'
	group by t.tag_typ_id 

