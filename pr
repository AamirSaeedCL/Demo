
SELECT
   TOP 10000
   [cnsmr_id],
   [cnsmr_nm_cmmrcl_txt],
   [cnsmr_brth_dt],
   [cnsmr_email_txt],
   [cnsmr_email_cnfrmtn_dt],
   [cnsmr_nm_frst_txt],
   [wrkgrp_id],
   [cnsmr_email_vldty_cd],
   [cnsmr_nm_lst_txt],
   [cnsmr_nm_mddl_txt],
   [cnsmr_nm_prfx_txt],
   [upsrt_dttm],
   0 AS IsMaster,
   1 AS IsActive,
   DATENAME(WEEKDAY, GETDATE()) AS DayOfWeek
FROM
   [DBO].[CNSMR]
 
WHERE
    [upsrt_dttm] > CAST(
        '@{activity('Lookup the pipeline log table to fetch lastrundatetime').output.value[0].LastRunDateTime}' AS DATETIME2)
