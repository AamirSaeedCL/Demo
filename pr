using System.Diagnostics;

public void DeployDacPacUsingSqlPackage(string dacPacPath, string connectionString)
{
    var process = new Process();
    process.StartInfo.FileName = "SqlPackage.exe";
    process.StartInfo.Arguments = $"/Action:Publish /SourceFile:\"{dacPacPath}\" /TargetConnectionString:\"{connectionString}\"";
    process.StartInfo.RedirectStandardOutput = true;
    process.StartInfo.UseShellExecute = false;
    process.StartInfo.CreateNoWindow = true;

    process.Start();

    string result = process.StandardOutput.ReadToEnd();
    process.WaitForExit();

    Console.WriteLine(result);
}
