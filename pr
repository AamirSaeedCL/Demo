CREATE PROCEDURE [dbo].[usp_Customer_Ingestion_UpdateCustomerIsValidEmail]
    @IsValidEmailData [dbo].[CustomerIsValidEmailTableType] READONLY
AS
BEGIN
    SET NOCOUNT ON;
    
    UPDATE c
    SET 
        c.IsValidEmail = 1
    FROM dbo.Customer c
    INNER JOIN @IsValidEmailData i ON c.CustomerId = i.CustomerId
    
END;
