
SELECT TOP (1000) [ConsumerID]
      ,[GroupID]
      ,[Confirmed]
      ,[DateModified]
  FROM [dbo].[ConsumerGroup]
  Order by DateModified Desc

SELECT TOP (1000) [CustomerId]
      ,[CustomerGroupId]
      ,[Title]
      ,[FirstName]
      ,[LastName]
      ,[MiddleName]
      ,[CommercialName]
      ,[DateofBirth]
      ,[Email]
      ,[EmailConfirmedDate]
      ,[IsMaster]
      ,[IsActive]
      ,[UpsertDateTime]
      ,[Ownership]
      ,[OwnershipDate]
      ,[IsBankHoliday]
      ,[DayOfWeek]
      ,[IsValidEmail]
      ,[LegacyId]
      ,[IsValidMobile]
  FROM [dbo].[Customer]
