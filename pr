CREATE TABLE [stg].[CustomerTags] (
    [CustomerId]          BIGINT           NOT NULL,
    [LegacyId]            BIGINT           NULL,
    [CustomerGroupId]     UNIQUEIDENTIFIER NULL,
        
     -- DD Tag       
    [DDTag]            VARCHAR (200)    NULL,
    [DateDDTagAssigned]   DATETIME2 (7)    NULL,
    [DateDDTagRemoved]    DATETIME2 (7)    NULL,
    [DDTagActive]         BIT              NOT NULL,
