-- Enable change tracking
IF NOT EXISTS (SELECT 1 FROM sys.change_tracking_tables WHERE object_id = OBJECT_ID('[dbo].[Account]'))
BEGIN
    ALTER TABLE [dbo].[Account] ENABLE CHANGE_TRACKING;
END
Go


